{
  "expo": {
    "name": "Kyro",
    "slug": "kyro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "solanaapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.test.solanaapp",
      "associatedDomains": [],
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSCameraUsageDescription": "This app needs access to the camera to take photos of e-waste items for AI analysis.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select images of e-waste items for AI analysis."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.test.solanaapp",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "extra": {
      "privyAppId": "cmea18uo900csl80c6z5obyb1",
      "privyClientId": "client-WY6PnvcfMUcMv3Fa8Ma3CxMKUfSNfptShSz82qmZk2o4s",
      "passkeyAssociatedDomain": ""
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-apple-authentication",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "17.5"
          },
          "android": {
            "compileSdkVersion": 35
          }
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-image-picker",
        {
          "photosPermission": "This app needs access to your photo library to select images of e-waste items for AI analysis.",
          "cameraPermission": "This app needs access to the camera to take photos of e-waste items for AI analysis."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
